{
  "name": "oradew-vscode",
  "displayName": "Oradew",
  "description": "Develop Oracle (PL/SQL) project in VS Code.",
  "version": "0.3.10",
  "publisher": "mp",
  "engines": {
    "vscode": "^1.30.0"
  },
  "license": "MIT",
  "categories": [
    "Other",
    "Extension Packs"
  ],
  "bin": {
    "oradew": "./out/cli.js"
  },
  "keywords": [
    "plsql",
    "PL/SQL",
    "oracle",
    "workspace"
  ],
  "homepage": "https://github.com/mickeypearce/oradew-vscode",
  "repository": {
    "type": "git",
    "url": "https://github.com/mickeypearce/oradew-vscode.git"
  },
  "bugs": {
    "url": "https://github.com/mickeypearce/oradew-vscode/issues"
  },
  "icon": "images/oradew_icon_128.png",
  "activationEvents": [
    "workspaceContains:dbconfig.json",
    "onCommand:oradew.initProjectTask",
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "menus": {
      "editor/title": [
        {
          "command": "oradew.compileFileTask",
          "when": "inOradewProject && resourceLangId == plsql",
          "group": "navigation@1",
          "alt": "oradew.compileObjectTask"
        },
        {
          "command": "oradew.deployTaskFile",
          "when": "inOradewProject && resourceLangId == plsql",
          "group": "navigation@2"
        }
      ],
      "editor/context": [
        {
          "command": "oradew.compileFileTask",
          "when": "inOradewProject && resourceLangId == plsql && !editorHasSelection",
          "group": "Oradew"
        },
        {
          "command": "oradew.exportFileTask",
          "when": "inOradewProject && resourceLangId == plsql && !editorHasSelection",
          "group": "Oradew"
        },
        {
          "command": "oradew.exportObjectTask",
          "when": "inOradewProject && resourceLangId == plsql && editorHasSelection",
          "group": "Oradew"
        },
        {
          "command": "oradew.compileObjectTask",
          "when": "inOradewProject && resourceLangId == plsql && editorHasSelection",
          "group": "Oradew"
        },
        {
          "command": "oradew.generateTask",
          "when": "inOradewProject && resourceLangId == plsql && editorHasSelection",
          "group": "Oradew"
        }
      ],
      "explorer/context": [
        {
          "command": "oradew.initProjectTask",
          "group": "Oradew@1",
          "category": "Oradew"
        },
        {
          "command": "oradew.compileTask",
          "group": "Oradew@2",
          "category": "Oradew",
          "when": "inOradewProject"
        },
        {
          "command": "oradew.testTask",
          "group": "Oradew@3",
          "category": "Oradew",
          "when": "inOradewProject"
        },
        {
          "command": "oradew.packageTask",
          "group": "Oradew@4",
          "category": "Oradew",
          "when": "inOradewProject"
        }
      ]
    },
    "keybindings": [
      {
        "command": "oradew.compileObjectTask",
        "key": "ctrl+enter",
        "when": "inOradewProject"
      },
      {
        "command": "oradew.deployTaskFile",
        "key": "f5",
        "when": "inOradewProject"
      },
      {
        "command": "oradew.exportTask",
        "key": "shift+f6",
        "when": "inOradewProject"
      },
      {
        "command": "oradew.compileTask",
        "key": "f6",
        "when": "inOradewProject"
      },
      {
        "command": "oradew.packageTask",
        "key": "f9",
        "when": "inOradewProject"
      },
      {
        "command": "oradew.packageDeltaTask",
        "key": "shift+f9",
        "when": "inOradewProject"
      },
      {
        "command": "workbench.action.tasks.reRunTask",
        "key": "ctrl+alt+enter",
        "when": "inOradewProject"
      }
    ],
    "problemMatchers": [
      {
        "name": "oracle-plsql",
        "owner": "external",
        "fileLocation": [
          "absolute"
        ],
        "pattern": [
          {
            "regexp": "^.*\\$(.*)$",
            "file": 1
          },
          {
            "regexp": "^(\\d+)\\/(\\d+)\\s+(WARNING|ERROR|INFO)\\s+(.*)$",
            "line": 1,
            "column": 2,
            "severity": 3,
            "message": 4,
            "loop": true
          }
        ],
        "background": {
          "activeOnStart": true,
          "beginsPattern": "^.*Starting compilation...$",
          "endsPattern": "Compilation complete."
        }
      }
    ],
    "commands": [
      {
        "command": "oradew.setDbEnvironment",
        "title": "Set DB Enviroment",
        "category": "Oradew"
      },
      {
        "command": "oradew.clearDbEnvironment",
        "title": "Clear DB Enviroment",
        "category": "Oradew"
      },
      {
        "command": "oradew.initProjectTask",
        "title": "Initialize Workspace/Version",
        "category": "Oradew"
      },
      {
        "command": "oradew.createProjectTask",
        "title": "Import All Source from DB",
        "category": "Oradew"
      },
      {
        "command": "oradew.deployTaskFile",
        "title": "Run Current File As Script",
        "category": "Oradew",
        "icon": {
          "light": "images/run_icon.png",
          "dark": "images/run_icon.png"
        }
      },
      {
        "command": "oradew.compileTask",
        "title": "Compile Changes to DB",
        "category": "Oradew"
      },
      {
        "command": "oradew.compileAllTask",
        "title": "Compile All Source to DB",
        "category": "Oradew"
      },
      {
        "command": "oradew.compileFileTask",
        "title": "Compile Current File",
        "category": "Oradew",
        "icon": {
          "light": "images/compile_icon.png",
          "dark": "images/compile_icon_alt.png"
        }
      },
      {
        "command": "oradew.compileObjectTask",
        "title": "Run Selected Statement",
        "category": "Oradew",
        "icon": {
          "light": "images/compile_icon_alt.png",
          "dark": "images/compile_icon.png"
        }
      },
      {
        "command": "oradew.compileOnSaveTask",
        "title": "Toggle Compile Watch",
        "category": "Oradew"
      },
      {
        "command": "oradew.exportTask",
        "title": "Import Changes from DB",
        "category": "Oradew"
      },
      {
        "command": "oradew.exportFileTask",
        "title": "Import Current File",
        "category": "Oradew"
      },
      {
        "command": "oradew.exportObjectTask",
        "title": "Import Selected Object",
        "category": "Oradew"
      },
      {
        "command": "oradew.testTask",
        "title": "Run Tests",
        "category": "Oradew"
      },
      {
        "command": "oradew.packageTask",
        "title": "Package",
        "category": "Oradew"
      },
      {
        "command": "oradew.packageDeltaTask",
        "title": "Package Delta",
        "category": "Oradew"
      },
      {
        "command": "oradew.deployTask",
        "title": "Deploy",
        "category": "Oradew"
      },
      {
        "command": "oradew.generateTask",
        "title": "Generate...",
        "category": "Oradew"
      }
    ],
    "jsonValidation": [
      {
        "fileMatch": "dbconfig.json",
        "url": "./resources/dbconfig-schema.json"
      },
      {
        "fileMatch": "oradewrc.json",
        "url": "./resources/oradewrc-schema.json"
      },
      {
        "fileMatch": "oradewrc.*.json",
        "url": "./resources/oradewrc-schema.json"
      },
      {
        "fileMatch": "oradewrc-generate.json",
        "url": "./resources/oradewrc-generate-schema.json"
      }
    ],
    "configuration": [
      {
        "type": "object",
        "title": "Oradew Settings",
        "properties": {
          "oradew.generatorConfigFile": {
            "type": "string",
            "default": "${workspaceFolder}/oradewrc-generate.json",
            "required": false,
            "description": "The path to the code generator configuration file"
          },
          "oradew.workspaceConfigFile": {
            "type": "string",
            "default": "${workspaceFolder}/oradewrc.json",
            "required": false,
            "description": "The path to the workspace configuration file"
          },
          "oradew.databaseConfigFile": {
            "type": "string",
            "default": "${workspaceFolder}/dbconfig.json",
            "required": false,
            "description": "The path to the database configuration file"
          },
          "oradew.chatty": {
            "type": "boolean",
            "default": false,
            "required": false,
            "description": "Enable informational output (in Gulp tasks)"
          }
        }
      }
    ]
  },
  "extensionDependencies": [
    "xyz.plsql-language",
    "mp.vscode-oracle-format"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm run precompile && tsc -p ./",
    "watch": "npm run precompile && tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "utest": "npm run compile && node ./node_modules/mocha/bin/mocha --ui tdd out/test/*test.js",
    "build": "vsce package",
    "publish": "vsce publish",
    "precompile": "node ./scripts/precompile.js",
    "install-cli": "npm install -g ./ --ignore-scripts --no-optional",
    "fix": "rd node_modules /S /Q & del package-lock.json & npm install --no-optional"
  },
  "devDependencies": {
    "@types/gulp": "^4.0.6",
    "@types/lodash": "^4.14.129",
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.14.6",
    "@types/oracledb": "^1.11.34",
    "mocha": "^5.2.0",
    "tslint": "^5.16.0",
    "typescript": "^2.9.2",
    "vsce": "^1.61.0",
    "vscode": "^1.1.34"
  },
  "dependencies": {
    "cli-table": "^0.3.1",
    "command-exists": "^1.2.8",
    "commander": "^2.20.0",
    "del": "4.0.0",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.4",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-convert-encoding": "^1.0.0",
    "gulp-data": "^1.2.1",
    "gulp-git": "^2.5.2",
    "gulp-insert": "^0.5.0",
    "gulp-multi-dest": "^1.3.7",
    "gulp-template": "^4.0.0",
    "gulp-todo": "^5.5.0",
    "inquirer": "^6.3.1",
    "lodash": "^4.17.10",
    "nedb": "^1.8.0",
    "oracledb": "3.1.1",
    "vinyl-map2": "^1.2.1",
    "vscode-extension-telemetry": "^0.1.1",
    "yargs": "^4.8.1"
  }
}
